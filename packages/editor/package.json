{
  "name": "@clutter/editor",
  "version": "0.0.0",
  "private": true,
  "description": "Behavioral editing engine for Clutter - TipTap-based editor with plugins and behaviors",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./types": {
      "types": "./types/index.ts",
      "import": "./types/index.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "eslint": "^8.57.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@clutter/shared": "*",
    "@clutter/ui": "*",
    "@tiptap/core": "^2.1.13",
    "@tiptap/extension-bullet-list": "^2.27.1",
    "@tiptap/extension-gapcursor": "^2.1.13",
    "@tiptap/extension-hard-break": "^2.1.13",
    "@tiptap/extension-highlight": "^2.1.13",
    "@tiptap/extension-history": "^2.1.13",
    "@tiptap/extension-link": "^2.1.13",
    "@tiptap/pm": "^2.1.13",
    "@tiptap/react": "^2.1.13",
    "@tiptap/suggestion": "^2.1.13",
    "tippy.js": "^6.3.7"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  }
}

